{
  "name": "Flutter Cubit",
  "needSubDir": true,
  "subDirNameCase": "snakeCase",
  "foldersFilesNamesReplacer": [
    {
      "case": "snakeCase",
      "nameToReplace": "key",
      "useSubDirName": true
    }
  ],
  "filesContentReplacer": [
    {
      "case": "snakeCase",
      "nameToReplace": "<{key_sc}>",
      "useSubDirName": true
    },
    {
      "case": "pascalCase",
      "nameToReplace": "<{key}>",
      "useSubDirName": true
    }
  ]
}
